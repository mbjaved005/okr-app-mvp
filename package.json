    "@netlify/plugin-nextjs": "^5.9.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.1",
    "cloudinary": "^2.5.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
